{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "jsx-self-close": false,
    "jsx-wrap-multiline": false,
    "no-consecutive-blank-lines": true,
    "no-empty": false,
    "no-implicit-dependencies": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  }
}